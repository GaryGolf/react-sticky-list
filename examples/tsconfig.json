{
  "compilerOptions": {
    "sourceMap": true,
    "module": "commonjs",
    "jsx": "react",
    "outDir": "./build/",
    "target": "es5",
    "baseUrl": "./src",
    "experimentalDecorators": true,
    "removeComments": true,
    "declaration": false,
    "lib": ["esnext", "dom"]
  },
  "include": ["./src/**/*"],
  "exclude": ["build", "node_modules"]
}